{
  "name": "@millennicare/nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@millennicare/api": "workspace:^",
    "@millennicare/db": "workspace:^",
    "@millennicare/lib": "workspace:^",
    "@millennicare/ui": "workspace:^",
    "@millennicare/validators": "workspace:^",
    "@radix-ui/react-icons": "^1.3.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.20.5",
    "@tanstack/react-query-devtools": "^5.20.5",
    "@tanstack/react-query-next-experimental": "5.20.5",
    "@trpc/client": "11.0.0-next-beta.283",
    "@trpc/react-query": "11.0.0-next-beta.283",
    "@trpc/server": "11.0.0-next-beta.283",
    "@vercel/speed-insights": "^1.0.9",
    "date-fns": "^3.3.1",
    "iron-session": "^8.0.1",
    "next": "^14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "^3.2.0",
    "sharp": "^0.33.2",
    "superjson": "2.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@millennicare/eslint-config": "workspace:^",
    "@millennicare/prettier-config": "workspace:^",
    "@millennicare/tailwind-config": "workspace:^",
    "@millennicare/tsconfig": "workspace:^",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@millennicare/eslint-config/base",
      "@millennicare/eslint-config/nextjs",
      "@millennicare/eslint-config/react"
    ]
  },
  "prettier": "@millennicare/prettier-config"
}
